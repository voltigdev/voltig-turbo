{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "prebuild:development": {
      "cache": false
    },
    "prebuild:production": {
      "cache": false
    },
    "run:android:development": {
      "cache": false
    },
    "run:android:production": {
      "cache": false
    },
    "run:ios:development": {
      "cache": false
    },
    "run:ios:production": {
      "cache": false
    },
    "check": {
      "dependsOn": ["^topo"],
      "outputLogs": "new-only"
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "ios": {
      "cache": false,
      "persistent": true
    },
    "android": {
      "cache": false,
      "persistent": true
    },
    "push": {
      "cache": false,
      "interactive": true
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "db-generate:development": {
      "cache": false,
      "interactive": true
    },
    "db-generate:production": {
      "cache": false,
      "interactive": true
    },
    "migrate:development": {
      "cache": false
    },
    "migrate:production": {
      "cache": false
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "AUTH_REDIRECT_PROXY_URL",
    "AUTH_SECRET",
    "PORT"
  ],
  "globalPassThroughEnv": ["NODE_ENV"]
}
